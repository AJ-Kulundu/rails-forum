<div class= "container flex space-x-8">
<%= link_to sanitize("&larr; Back to Communities"), communities_path %>
<h1 class="text-4xl font-bold text-center"> <%= @community.name %> </h1>
</div>

<div class="flex flex-col space-y-4">
<% @community.members.each do |member|%>
<div class="flex space-x-4">
<%= member.user.name  %>
<%= member.role.capitalize%>

</div>
<%end%>
</div>